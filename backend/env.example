# Server Configuration
PORT=3000
NODE_ENV=development

# Demo Mode (set to true to use mock data)
DEMO_MODE=false

# Redis Configuration (optional - will fallback to in-memory cache if not available)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# API Keys (add your keys here)
# eNAM API Key - Register at https://enam.gov.in
ENAM_API_KEY=your_enam_api_key_here

# AGMARKNET API Key - Register at https://agmarknet.gov.in
AGMARKNET_API_KEY=your_agmarknet_api_key_here

# Data.gov.in API Key - Register at https://data.gov.in
DATAGOV_API_KEY=your_datagov_api_key_here

# Rate Limiting
RATE_LIMIT_DELAY_MS=1000
CACHE_TTL_SECONDS=3600

# Logging
LOG_LEVEL=info
